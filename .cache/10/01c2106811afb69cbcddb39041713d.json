lumn":8},"generated":{"line":226,"column":14}},{"source":"style.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":227,"column":2}},{"source":"style.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":227,"column":28}},{"source":"style.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":228,"column":2}},{"source":"style.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":228,"column":19}}],"sources":{"style.scss":":root {\n  --primary-color: #000;\n  --secondary-color: #fff;\n}\n\n/* .items {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  gap: 20px;\n  font-family: Arial, Helvetica, sans-serif;\n  padding: 20px;\n}\n.item {\n  flex: 1;\n  min-width: 200px;\n  display: grid;\n  grid-template-rows: repeat(4, auto);\n  border-radius: 3px;\n  padding: 30px 20px;\n  box-shadow: 0 3px 4px rgba(0, 0, 0, 0.2);\n  flex-direction: column;\n  text-align: center;\n  font-size: 0.8rem;\n}\n\n\n.item .Name {\n  font-size: 1.2rem;\n  font-weight: 600;\n}\n.item .Role {\n  font-size: 1rem;\n  font-weight: 500;\n  margin-bottom: 20px;\n}\n.item .Bio {\n  margin-bottom: 10px;\n}\n.item .Button_text {\n  margin-top: 20px;\n} */\n\n.item a {\n  text-decoration: none;\n  font-size: 1rem !important;\n  background: rgb(var(--primary-color));\n  padding: 15px 20px;\n  color: rgb(var(--secondary-color));\n  border-radius: 2px;\n  display: inline-block;\n  font-weight: 600;\n  transition: color 0.3s, background-color 0.3s;\n  &:hover {\n    background: #000;\n    color: #fff;\n  }\n}\n\n/* list-1*/\n\n.list-1,\n.list-1 * {\n  box-sizing: border-box;\n  font-family: Arial, Helvetica, sans-serif;\n}\n\n.list-1 {\n  display: grid;\n  grid-template-columns: 1fr 1fr 1fr;\n  grid-template-rows: flow;\n  gap: 3px;\n}\n.list-1 .item {\n  border-radius: 4px;\n  display: grid;\n  grid-template-rows: 300px repeat(3, auto);\n  align-items: end;\n  padding: 20px;\n  position: relative;\n  color: #fff;\n}\n.list-1 .item div:not(.Image_url) {\n  z-index: 100;\n}\n.list-1 .item .Event-Name {\n  font-size: 2rem;\n  margin-bottom: 10px;\n  color: rgb(var(--primary-color));\n  font-weight: 600;\n  margin-top: 20px;\n  word-break: break-word;\n}\n.list-1 .item .Date,\n.list-1 .item .Time {\n  color: rgb(var(--secondary-color));\n  filter: invert(1);\n}\n.list-1 .item .Button_text {\n  margin-top: 20px;\n}\n.list-1 .item .Image_url {\n  height: 100%;\n  width: 100%;\n  position: absolute;\n}\n.list-1 .item .Image_url img {\n  object-fit: cover;\n  width: 100%;\n  height: 100%;\n}\n.list-1 .item .Image_url::after {\n  content: \"\";\n  height: 100%;\n  width: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  background: linear-gradient(\n    rgba(var(--secondary-color), 0),\n    rgba(var(--secondary-color), 1) 80%\n  );\n  // background: $c;\n}\n\n/* calendar-1 */\n\n.calendar-1,\n.calendar-1 * {\n  box-sizing: border-box;\n  font-family: Arial, Helvetica, sans-serif;\n}\n.calendar-1 {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(330px, 1fr));\n  padding: 30px;\n  place-items: center;\n  align-items: flex-start;\n  gap: 30px;\n\n  .item {\n    width: 100%;\n    margin: 0 1rem 1rem 0;\n    box-shadow: 0 4px 4px rgba(0, 0, 0, 0.2);\n    border-radius: 4px;\n    word-break: break-word;\n    display: grid;\n    grid-template-rows: 150px auto;\n    grid-template-columns: repeat(3, 1fr);\n    align-items: center;\n    padding: 20px;\n    row-gap: 20px;\n    position: relative;\n    &:first-child {\n      &::after {\n        content: \"Next event\";\n        display: block;\n        position: absolute;\n        top: 20px;\n        left: 20px;\n        background: rgb(var(--secondary-color));\n        color: rgb(var(--primary-color));\n        padding: 6px 10px;\n        font-weight: 600;\n        border-radius: 3px;\n      }\n    }\n    .Date,\n    .Time,\n    .Location {\n      text-align: center;\n      justify-self: flex-start;\n      position: relative;\n      align-items: center;\n      display: flex;\n      grid-column: 1 / span 3;\n      margin-bottom: -10px;\n      font-size: 0.9rem;\n    }\n    // div:not(.Image_url) {\n    //   padding: 10px;\n    // }\n    .Date::before,\n    .Time::before,\n    .Location::before {\n      display: inline-block;\n      margin-right: 5px;\n      content: \" \";\n      background-size: contain;\n      background-position: center;\n      height: 16px;\n      width: 16px;\n    }\n    .Date {\n      &::before {\n        background-image: url(\"./assets/calendar.svg\");\n      }\n    }\n    .Time {\n      &::before {\n        background-image: url(\"./assets/alarm-clock.svg\");\n      }\n    }\n    .Location {\n      &::before {\n        background-image: url(\"./assets/map.svg\");\n      }\n    }\n    .Description {\n      grid-column: 1 / span 3;\n      opacity: 0.85;\n    }\n    .Event-Name {\n      grid-row: 2;\n      grid-column: 1 / span 3;\n      font-weight: 600;\n      padding: 10px 0;\n      font-size: 20px;\n      margin-top: 10px;\n      margin-bottom: -20px;\n    }\n\n    .Button_text {\n      grid-column: 1 / span 3;\n      justify-self: flex-end;\n    }\n    .Image_url {\n      grid-row: 1;\n      grid-column: 1 / span 3;\n      width: calc(100% + 40px);\n      height: calc(100% + 40px);\n      margin-left: -20px;\n      img {\n        width: 100%;\n        height: 100%;\n        border-radius: 4px 4px 0 0;\n        object-fit: cover;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"279209361bd59257ea69229abb3e7d84","cacheData":{"env":{}}}